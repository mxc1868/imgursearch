<!DOCTYPE HTML>
<html>
    <head>
        <%- include partials/header %>
        <link rel="stylesheet" href="/css/empty.css">
    </head>
    <body draggable="false">
        <%- include partials/nav %>
        <div draggable="false" id="searchResult" class="container">
            <div class="grid" data-masonry='{ "itemSelector": ".grid-item", "columnWidth": 350 }'>
                <% if (data) {%>
                    <%- include imagesResult %>
                <% } else {%>
                <form class="index-form">
                    <input class="form-control" type="text" placeholder="Image Search" aria-label="Search" name="q">
                    <button class="btn btn-outline-success index-btn" type="submit">Search</button>
                </form>
                <% } %>
            </div>
        </div>
        <script src="/js/jquery.lazyloadxt.js"></script>
        <script src="/js/drag.js"></script>
        <script src="/js/nextpage.js"></script>
        <%- include partials/footer %>
    </body>
</html>
